<h1>Editing matricula</h1>

<% form_for(@matricula) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :alumno_id %><br />
    <%=Alumno.find(@matricula.alumno_id).nombre %>
  </p>
  <p>
    <%= f.label :curso_id %><br />
    <%= f.select :curso_id, nombre_curso  %>
     <td><%= f.submit 'ver_horario' %></td>

  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @matricula %> |
<%= link_to 'Back', matriculas_path %>
