<h1>New horario</h1>

<% form_for(@horario) do |f| %>
  <%= f.error_messages %>
<p>
    <%= f.label :curso_id %><br />
    <%= f.text_field :curso_id %>
  </p>

  <p>
    <%= f.label :dia %><br />
    <%= f.text_field :dia %>
  </p>
  <p>
    <%= f.label :hora %><br />
    <%= f.text_field :hora %>
  </p>
  <p>
    <%= f.label :duracion %><br />
    <%= f.text_field :duracion %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', horarios_path %>
